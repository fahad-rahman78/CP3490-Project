<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>

<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21"
      fx:controller="com.eventsystem.controller.ViewRegistrationsController"
      spacing="10" style="-fx-padding: 15; -fx-pref-width: 400; -fx-pref-height: 300;">

    <Label text="Registered Students" textFill="#333333">
        <font>
            <Font name="System Bold" size="16.0" />
        </font>
    </Label>

    <ListView fx:id="registrationListView" VBox.vgrow="ALWAYS"
              style="-fx-border-color: #cccccc; -fx-border-radius: 5;"/>

</VBox>